{% extends 'layout.njk' %}

{% block content %}

<section class="cards">
{% for recipe in recipes %}
        <div class="card">
            <img src="{{recipe.image}}">
            <p class="card-title">{{recipe.title}}</p>
            <p class="card-title card-author">{{recipe.author}}</p>
        </div>
{% endfor %}
</section>

<script src="/scripts.js"></script>

{% endblock content %}